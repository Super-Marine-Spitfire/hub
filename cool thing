noStroke();
for (var x = 0; x < 400; x += 5) {
    for (var y = 0; y < 400; y += 5) {
        if (((x/5)*(y/5)) > 999) {
            fill(0, 0, 255);
        } else {
            if (((x/5)*(y/5)) > 99) {
                fill(0, 255, 0);
            } else {
                if (((x/5)*(y/5)) > 9) {
                    fill(255, 0, 0);
                } else {
                    fill(0, 0, 0);
                }
            }
        }
        rect(x, y, 5, 5);
    }
}
